# Backend API Configuration
VITE_API_BASE_URL=http://localhost:5000

# Firebase Configuration
# Get these from Firebase Console > Project Settings > General > Your apps > Web app
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project-id.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_firebase_app_id

# Google Maps Configuration (Optional - Leaflet is used by default)
# Get your API key from: https://console.cloud.google.com/google/maps-apis
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Google Maps Map ID (Optional - For advanced map styling)
# Create a Map ID in: https://console.cloud.google.com/google/maps-apis/studio/maps
VITE_MAP_ID=your_map_id_here
